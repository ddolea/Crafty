<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#8FBC8F"
    android:columnCount="2"
    android:orientation="horizontal"
    android:rowCount="2"
    tools:ignore="MissingDefaultResource,NewApi">

    <Button
        android:id="@+id/buttonClearDataM"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="147dp"
        android:layout_marginEnd="148dp"
        android:backgroundTint="@color/design_default_color_secondary_variant"
        android:onClick="clearUserData"
        android:text="Delete this"
        android:textColor="#FFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/lamai"
        android:layout_width="180dp"
        android:layout_height="204dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="100dp"
        android:layout_marginEnd="11dp"
        android:background="#D2B48C"
        app:layout_constraintEnd_toStartOf="@+id/grosbi"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/lamai"
        tools:ignore="ContentDescription" />

    <ImageView
        android:id="@+id/grosbi"
        android:layout_width="180dp"
        android:layout_height="204dp"
        android:layout_marginStart="11dp"
        android:layout_marginTop="100dp"
        android:layout_marginEnd="20dp"
        android:background="#D2B48C"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/lamai"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/grosbi"
        tools:ignore="ContentDescription" />

    <ImageView
        android:id="@+id/pepsi"
        android:layout_width="180dp"
        android:layout_height="204dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="11dp"
        android:background="#D2B48C"
        app:layout_constraintEnd_toStartOf="@+id/vinuri"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/lamai"
        app:srcCompat="@drawable/pepsi"
        tools:ignore="ContentDescription" />

    <ImageView
        android:id="@+id/vinuri"
        android:layout_width="180dp"
        android:layout_height="204dp"
        android:layout_marginStart="11dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="20dp"
        android:background="#D2B48C"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/pepsi"
        app:layout_constraintTop_toBottomOf="@+id/grosbi"
        app:srcCompat="@drawable/vinuri"
        tools:ignore="ContentDescription" />

    <Button
        android:id="@+id/buttonFinish"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="105dp"
        android:layout_marginBottom="38dp"
        android:background="#FFC0CB"
        android:text="Finish"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        tools:ignore="TextContrastCheck" />
</androidx.constraintlayout.widget.ConstraintLayout>
